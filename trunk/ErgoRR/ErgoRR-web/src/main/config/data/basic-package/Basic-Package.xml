                </rim:ValueList>
            </rim:Slot>
            <rim:Name>
                <rim:LocalizedString value="TM_GeometricPrimitive" />
            </rim:Name>
            <rim:Description>
                <rim:LocalizedString xml:lang="en"
        value="Root temporal primitive data type. Values must be valid GML 3.1 (OGC 03-105r1, ISO/CD 19136) representations. Any primitive temporal subtype is a permissible value. Following ISO 8601, the Gregorian calendar with UTC is the default reference system."/>
            </rim:Description>
            <rim:ClassificationNode code="TM_Instant"
        parent="urn:ogc:def:dataType:ISO-19108:2002:TM_GeometricPrimitive" 
        id="urn:ogc:def:dataType:ISO-19108:2002:TM_Instant">
                <rim:Name>
                    <rim:LocalizedString xml:lang="en" value="TM_Instant"/>
                </rim:Name>
                <rim:Description>
                    <rim:LocalizedString xml:lang="en"
            value="The gml:TimeInstant element denotes a position in time. In practice, an instant is an interval whose duration is less than the resolution of the time scale."/>
                </rim:Description>
            </rim:ClassificationNode>
            <rim:ClassificationNode code="TM_Period"
        parent="urn:ogc:def:dataType:ISO-19108:2002:TM_GeometricPrimitive" 
        id="urn:ogc:def:dataType:ISO-19108:2002:TM_Period">
                <rim:Name>
                    <rim:LocalizedString value="TM_Period"/>
                </rim:Name>
                <rim:Description>
                    <rim:LocalizedString
            value="The gml:TimePeriod element denotes an extent in time. It is an open interval bounded by beginning and end points (i.e. instants), and has a duration."/>
                </rim:Description>
            </rim:ClassificationNode>
